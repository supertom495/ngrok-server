
server {
        listen          80;
        listen          [::]:80;
        server_name     kgalaxy-burwood.api.redpayments.com.au www.kgalaxy-burwood.api.redpayments.com.au;
        location / {
                proxy_pass http://www.kgalaxy-burwood.api.redpayments.com.au:9059; 
        }    
}    


---------------------------------------------------------------------------------------

docker build -t kgalaxy-burwood.api.redpayments.com.au:latest .

---------------------------------------------------------------------------------------

docker run -t --name kgalaxy-burwood.api.redpayments.com.au -p 9059:9059 -p 9060:9060 kgalaxy-burwood.api.redpayments.com.au:latest

---------------------------------------------------------------------------------------

