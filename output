
server {
        listen          80;
        listen          [::]:80;
        server_name     zhongyi.api.redpayments.com.au www.zhongyi.api.redpayments.com.au;
        location / {
                proxy_pass http://www.zhongyi.api.redpayments.com.au:9053; 
        }    
}    


---------------------------------------------------------------------------------------

docker build -t zhongyi.api.redpayments.com.au:latest .

---------------------------------------------------------------------------------------

docker run -t --name zhongyi.api.redpayments.com.au -p 9053:9053 -p 9054:9054 zhongyi.api.redpayments.com.au:latest

---------------------------------------------------------------------------------------

zhongyi.api.redpayments.com.au:9054
