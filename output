
server {
        listen          80;
        listen          [::]:80;
        server_name     dumplingco.api.redpayments.com.au www.dumplingco.api.redpayments.com.au;
        location / {
                proxy_pass http://www.dumplingco.api.redpayments.com.au:9069; 
        }    
}    


---------------------------------------------------------------------------------------

docker build -t dumplingco.api.redpayments.com.au:latest .

---------------------------------------------------------------------------------------

docker run -t --name dumplingco.api.redpayments.com.au -p 9069:9069 -p 9070:9070 dumplingco.api.redpayments.com.au:latest

---------------------------------------------------------------------------------------

dumplingco.api.redpayments.com.au:9070
