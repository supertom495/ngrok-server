
server {
        listen          80;
        listen          [::]:80;
        server_name     jackpothotpot.api.redpayments.com.au www.jackpothotpot.api.redpayments.com.au;
        location / {
                proxy_pass http://www.jackpothotpot.api.redpayments.com.au:9045; 
        }    
}    


---------------------------------------------------------------------------------------

docker build -t jackpothotpot.api.redpayments.com.au:latest .

---------------------------------------------------------------------------------------

docker run -t --name jackpothotpot.api.redpayments.com.au -p 9045:9045 -p 9046:9046 jackpothotpot.api.redpayments.com.au:latest

---------------------------------------------------------------------------------------

jackpothotpot.api.redpayments.com.au:9046
