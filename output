
server {
        listen          80;
        listen          [::]:80;
        server_name     chuanchuanhotpot.api.redpayments.com.au www.chuanchuanhotpot.api.redpayments.com.au;
        location / {
                proxy_pass http://www.chuanchuanhotpot.api.redpayments.com.au:9071; 
        }    
}    


---------------------------------------------------------------------------------------

docker build -t chuanchuanhotpot.api.redpayments.com.au:latest .

---------------------------------------------------------------------------------------

docker run -t --name chuanchuanhotpot.api.redpayments.com.au -p 9071:9071 -p 9072:9072 chuanchuanhotpot.api.redpayments.com.au:latest

---------------------------------------------------------------------------------------

chuanchuanhotpot.api.redpayments.com.au:9072
