
server {
        listen          80;
        listen          [::]:80;
        server_name     baodaotwkitchen.api.redpayments.com.au www.baodaotwkitchen.api.redpayments.com.au;
        location / {
                proxy_pass http://www.baodaotwkitchen.api.redpayments.com.au:9077; 
        }    
}    


---------------------------------------------------------------------------------------

docker build -t baodaotwkitchen.api.redpayments.com.au:latest .

---------------------------------------------------------------------------------------

docker run -t --name baodaotwkitchen.api.redpayments.com.au -p 9077:9077 -p 9078:9078 baodaotwkitchen.api.redpayments.com.au:latest

