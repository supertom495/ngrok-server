
server {
        listen          80;
        listen          [::]:80;
        server_name     baodaotaiwanesekitchen.api.redpayments.com.au www.baodaotaiwanesekitchen.api.redpayments.com.au;
        location / {
                proxy_pass http://www.baodaotaiwanesekitchen.api.redpayments.com.au:9077; 
        }    
}    


