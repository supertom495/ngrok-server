
server {
        listen          80;
        listen          [::]:80;
        server_name     nanjingburwood.api.redpayments.com.au www.nanjingburwood.api.redpayments.com.au;
        location / {
                proxy_pass http://www.nanjingburwood.api.redpayments.com.au:9073; 
        }    
}    


---------------------------------------------------------------------------------------

docker build -t nanjingburwood.api.redpayments.com.au:latest .

---------------------------------------------------------------------------------------

docker run -t --name nanjingburwood.api.redpayments.com.au -p 9073:9073 -p 9074:9074 nanjingburwood.api.redpayments.com.au:latest

