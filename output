
server {
        listen          80;
        listen          [::]:80;
        server_name     spicyworld.api.redpayments.com.au www.spicyworld.api.redpayments.com.au;
        location / {
                proxy_pass http://www.spicyworld.api.redpayments.com.au:9043; 
        }    
}    


---------------------------------------------------------------------------------------

docker build -t spicyworld.api.redpayments.com.au:latest .

---------------------------------------------------------------------------------------

docker run -t --name spicyworld.api.redpayments.com.au -p 9043:9043 -p 9044:9044 spicyworld.api.redpayments.com.au:latest

---------------------------------------------------------------------------------------

spicyworld.api.redpayments.com.au:9044
