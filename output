
server {
        listen          80;
        listen          [::]:80;
        server_name     hkyuebaby.api.redpayments.com.au www.hkyuebaby.api.redpayments.com.au;
        location / {
                proxy_pass http://www.hkyuebaby.api.redpayments.com.au:9031; 
        }    
}    


---------------------------------------------------------------------------------------

docker build -t hkyuebaby.api.redpayments.com.au:latest .

---------------------------------------------------------------------------------------

docker run -t --name hkyuebaby.api.redpayments.com.au -p 9031:9031 -p 9032:9032 hkyuebaby.api.redpayments.com.au:latest

---------------------------------------------------------------------------------------

hkyuebaby.api.redpayments.com.au:9032
