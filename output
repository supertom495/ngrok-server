
server {
        listen          80;
        listen          [::]:80;
        server_name     goodluckhotpot.api.redpayments.com.au www.goodluckhotpot.api.redpayments.com.au;
        location / {
                proxy_pass http://www.goodluckhotpot.api.redpayments.com.au:9049; 
        }    
}    


---------------------------------------------------------------------------------------

docker build -t goodluckhotpot.api.redpayments.com.au:latest .

---------------------------------------------------------------------------------------

docker run -t --name goodluckhotpot.api.redpayments.com.au -p 9049:9049 -p 9050:9050 goodluckhotpot.api.redpayments.com.au:latest

---------------------------------------------------------------------------------------

goodluckhotpot.api.redpayments.com.au:9050
