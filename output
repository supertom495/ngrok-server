
server {
        listen          80;
        listen          [::]:80;
        server_name     takasushi.api.redpayments.com.au www.takasushi.api.redpayments.com.au;
        location / {
                proxy_pass http://www.takasushi.api.redpayments.com.au:9065; 
        }    
}    


---------------------------------------------------------------------------------------

docker build -t takasushi.api.redpayments.com.au:latest .

---------------------------------------------------------------------------------------

docker run -t --name takasushi.api.redpayments.com.au -p 9065:9065 -p 9066:9066 takasushi.api.redpayments.com.au:latest

---------------------------------------------------------------------------------------

takasushi.api.redpayments.com.au:9066
