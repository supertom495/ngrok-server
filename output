
server {
        listen          80;
        listen          [::]:80;
        server_name     lilongchatswood.api.redpayments.com.au www.lilongchatswood.api.redpayments.com.au;
        location / {
                proxy_pass http://www.lilongchatswood.api.redpayments.com.au:9047; 
        }    
}    


---------------------------------------------------------------------------------------

docker build -t lilongchatswood.api.redpayments.com.au:latest .

---------------------------------------------------------------------------------------

docker run -t --name lilongchatswood.api.redpayments.com.au -p 9047:9047 -p 9048:9048 lilongchatswood.api.redpayments.com.au:latest

