
server {
        listen          80;
        listen          [::]:80;
        server_name     jiaojiaoxiaoyu.api.redpayments.com.au www.jiaojiaoxiaoyu.api.redpayments.com.au;
        location / {
                proxy_pass http://www.jiaojiaoxiaoyu.api.redpayments.com.au:9033; 
        }    
}    


---------------------------------------------------------------------------------------

docker build -t jiaojiaoxiaoyu.api.redpayments.com.au:latest .

---------------------------------------------------------------------------------------

docker run -t --name jiaojiaoxiaoyu.api.redpayments.com.au -p 9033:9033 -p 9034:9034 jiaojiaoxiaoyu.api.redpayments.com.au:latest

