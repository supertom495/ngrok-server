
server {
        listen          80;
        listen          [::]:80;
        server_name     api.xiyantong.pw www.api.xiyantong.pw;
        location / {
                proxy_pass http://www.api.xiyantong.pw:9014; 
        }    
}    


---------------------------------------------------------------------------------------

docker build -t api.xiyantong.pw:latest .

---------------------------------------------------------------------------------------

docker run -t --name api.xiyantong.pw -p 9014:9014 -p 9015:9015 api.xiyantong.pw:latest

---------------------------------------------------------------------------------------

api.xiyantong.pw:9015
