
server {
        listen          80;
        listen          [::]:80;
        server_name     mtoffice.xiyantong.pw www.mtoffice.xiyantong.pw;
        location / {
                proxy_pass http://www.mtoffice.xiyantong.pw:9018; 
        }    
}    


---------------------------------------------------------------------------------------

docker build -t mtoffice.xiyantong.pw:latest .

---------------------------------------------------------------------------------------

docker run -t --name mtoffice.xiyantong.pw -p 9018:9018 -p 9019:9019 mtoffice.xiyantong.pw:latest

---------------------------------------------------------------------------------------

mtoffice.xiyantong.pw:9019
