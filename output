
server {
        listen          80;
        listen          [::]:80;
        server_name     xiaochuanyue.api.redpayments.com.au www.xiaochuanyue.api.redpayments.com.au;
        location / {
                proxy_pass http://www.xiaochuanyue.api.redpayments.com.au:9041; 
        }    
}    


---------------------------------------------------------------------------------------

docker build -t xiaochuanyue.api.redpayments.com.au:latest .

---------------------------------------------------------------------------------------

docker run -t --name xiaochuanyue.api.redpayments.com.au -p 9041:9041 -p 9042:9042 xiaochuanyue.api.redpayments.com.au:latest

---------------------------------------------------------------------------------------

