
server {
        listen          80;
        listen          [::]:80;
        server_name     hotpotpalace.api.redpayments.com.au www.hotpotpalace.api.redpayments.com.au;
        location / {
                proxy_pass http://www.hotpotpalace.api.redpayments.com.au:9029; 
        }    
}    


---------------------------------------------------------------------------------------

docker build -t hotpotpalace.api.redpayments.com.au:latest .

---------------------------------------------------------------------------------------

docker run -t --name hotpotpalace.api.redpayments.com.au -p 9029:9029 -p 9030:9030 hotpotpalace.api.redpayments.com.au:latest

---------------------------------------------------------------------------------------

hotpotpalace.api.redpayments.com.au:9030
