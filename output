
server {
        listen          80;
        listen          [::]:80;
        server_name     warmingricenoodle.api.redpayments.com.au www.warmingricenoodle.api.redpayments.com.au;
        location / {
                proxy_pass http://www.warmingricenoodle.api.redpayments.com.au:9051; 
        }    
}    


---------------------------------------------------------------------------------------

docker build -t warmingricenoodle.api.redpayments.com.au:latest .

---------------------------------------------------------------------------------------

docker run -t --name warmingricenoodle.api.redpayments.com.au -p 9051:9051 -p 9052:9052 warmingricenoodle.api.redpayments.com.au:latest

---------------------------------------------------------------------------------------

warmingricenoodle.api.redpayments.com.au:9052
