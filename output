
server {
        listen          80;
        listen          [::]:80;
        server_name     zhuangyuanwonton.api.redpayments.com.au www.zhuangyuanwonton.api.redpayments.com.au;
        location / {
                proxy_pass http://www.zhuangyuanwonton.api.redpayments.com.au:9063; 
        }    
}    


---------------------------------------------------------------------------------------

docker build -t zhuangyuanwonton.api.redpayments.com.au:latest .

---------------------------------------------------------------------------------------

docker run -t --name zhuangyuanwonton.api.redpayments.com.au -p 9063:9063 -p 9064:9064 zhuangyuanwonton.api.redpayments.com.au:latest

