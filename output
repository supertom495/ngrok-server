
server {
        listen          80;
        listen          [::]:80;
        server_name     office.xiyantong.pw www.office.xiyantong.pw;
        location / {
                proxy_pass http://www.office.xiyantong.pw:9016; 
        }    
}    


---------------------------------------------------------------------------------------

docker build -t office.xiyantong.pw:latest .

---------------------------------------------------------------------------------------

docker run -t --name office.xiyantong.pw -p 9016:9016 -p 9017:9017 office.xiyantong.pw:latest

---------------------------------------------------------------------------------------

office.xiyantong.pw:9017
