
server {
        listen          80;
        listen          [::]:80;
        server_name     nanjingwaterloo.api.redpayments.com.au www.nanjingwaterloo.api.redpayments.com.au;
        location / {
                proxy_pass http://www.nanjingwaterloo.api.redpayments.com.au:9057; 
        }    
}    


---------------------------------------------------------------------------------------

docker build -t nanjingwaterloo.api.redpayments.com.au:latest .

---------------------------------------------------------------------------------------

docker run -t --name nanjingwaterloo.api.redpayments.com.au -p 9057:9057 -p 9058:9058 nanjingwaterloo.api.redpayments.com.au:latest

---------------------------------------------------------------------------------------

nanjingwaterloo.api.redpayments.com.au:9058
