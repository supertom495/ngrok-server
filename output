
server {
        listen          80;
        listen          [::]:80;
        server_name     longfengsuancaiyu.api.redpayments.com.au www.longfengsuancaiyu.api.redpayments.com.au;
        location / {
                proxy_pass http://www.longfengsuancaiyu.api.redpayments.com.au:9067; 
        }    
}    


---------------------------------------------------------------------------------------

docker build -t longfengsuancaiyu.api.redpayments.com.au:latest .

---------------------------------------------------------------------------------------

docker run -t --name longfengsuancaiyu.api.redpayments.com.au -p 9067:9067 -p 9068:9068 longfengsuancaiyu.api.redpayments.com.au:latest

---------------------------------------------------------------------------------------

